<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />

	<bean name="addressInMumbai"
		class="com.springdemos.springcore.autowiring.annotations.Address"
		p:housenumber="27" p:street="Main Street" p:city="Mumbai"/>
		
	<bean name="addressInChennai"
		class="com.springdemos.springcore.autowiring.annotations.Address"
		p:housenumber="145" p:street="Avenue Street" p:city="Chennai"/>		

	<bean name="emp"
		class="com.springdemos.springcore.autowiring.annotations.Employee" 
		p:id="1001" p:name="Max" p:age="36" />


</beans>